{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "siyazADF"
		},
		"ExcelSourceLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ExcelSourceLS'"
		},
		"SinkLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SinkLS'"
		},
		"ExcelSourceLS_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\Dell\\Desktop\\LTI\\Project Gladiator\\"
		},
		"ExcelSourceLS_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Dell"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "siyazADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SinkLS_connectionString": {
						"value": "[parameters('SinkLS_connectionString')]"
					},
					"ExcelSourceLS_password": {
						"value": "[parameters('ExcelSourceLS_password')]"
					},
					"ExcelSourceLS_properties_typeProperties_host": {
						"value": "[parameters('ExcelSourceLS_properties_typeProperties_host')]"
					},
					"ExcelSourceLS_properties_typeProperties_userId": {
						"value": "[parameters('ExcelSourceLS_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}